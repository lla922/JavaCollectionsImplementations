package ba.yzl.lib;
import ba.yzl.Exception.*;

public final class libUtils {
	
	public static void doSomething() throws MyCustomException { // diardan cagiracak olan kullaniciya bak dikkat et bu methodda mycustom exception firlatilabilir diyor 
				System.out.println("method doSomething() is running...");
				throw new MyCustomException("originated from doSomehing()");
		
		
	}
	
	public static void f() throws MyCustomException {
		System.out.println("method f() is running...");
		throw new MyCustomException("originated from f ()");
	}
	
	

}
